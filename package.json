{
  "name": "pomodoro",
  "version": "1.0.0",
  "main": "index.js",
  "author": "PakkuDon <pakku.pd@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "npm run copy && webpack-cli",
    "start:dev": "npm run copy && webpack-cli --watch",
    "copy": "cp src/chime.mp3 dist/",
    "lint": "eslint src",
    "prettier:list": "prettier --list-different \"src/**/*.js\"",
    "prettier:fix": "prettier --write --list-different \"src/**/*.js\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "audit": "rm -f package-lock.json && synp -s yarn.lock && npm audit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.2",
    "css-loader": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "gh-pages": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.15",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "style-loader": "^0.21.0",
    "synp": "^1.3.1",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5"
  }
}
